{{#each stations}}
  <section class="ui stacked segment">

    <h2 class="ui header">
      {{title}}
    </h2>
    <!-- station already is main object coming from "each station" statement on top -->
    {{> stationheader}}
    <a href="/station/{{id}}" class="ui icon button">
      <i class="icon folder open"></i>
    </a>
    <a href="/dashboard/deletestation/{{id}}" class="ui icon button" style="color: red">
      <i class="icon trash"></i>
    </a>
  </section>
{{/each}}
